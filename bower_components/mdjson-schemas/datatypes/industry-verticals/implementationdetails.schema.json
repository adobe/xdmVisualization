{
  "id": "_xdm.context.implementationdetails",
  "$schema": "http://json-schema.org/draft-06/schema#",
  "title": "Implementation details",
  "type": "object",
  "meta:extensible": true,
  "meta:status": "stable",
  "description": "Details about the SDK, library, or service used in an application or web page implementation of a service.",
  "meta:createdDate": "2021-02-19",
  "meta:xdmType": "object",
  "properties": {
    "name": {
      "title": "Name",
      "type": "string",
      "format": "uri",
      "description": "SDK or endpoint identifier. All SDKs or endpoints are identified through a URI, including extensions.",
      "meta:xdmType": "string",
      "meta:xdmField": "xdm:name",
      "tagNumber": 1
    },
    "version": {
      "title": "Version",
      "type": "string",
      "description": "The version identifier of the API, e.g h.18.",
      "meta:xdmType": "string",
      "meta:xdmField": "xdm:version",
      "tagNumber": 2
    },
    "environment": {
      "title": "Environment",
      "type": "string",
      "description": "The environment of the implementation",
      "enum": [
        "browser",
        "app",
        "server",
        "iot"
      ],
      "meta:xdmType": "string",
      "meta:xdmField": "xdm:environment",
      "tagNumber": 3
    }
  },
  "meta:xdmId": "https://ns.adobe.com/xdm/context/implementationdetails",
  "meta:altId": "_xdm.context.implementationdetails",
  "id": "_xdm.context.implementationdetails"
}
