{
  "id": "_xdm.mixins.healthcare-plan-details",
  "$schema": "http://json-schema.org/draft-06/schema#",
  "title": "Healthcare Plan Details",
  "type": "object",
  "meta:extensible": true,
  "meta:abstract": true,
  "meta:tags": {
    "industry": [
      "healthcare_services"
    ]
  },
  "description": "Details of a Health Insurance plan provided by an organization.",
  "meta:status": "experimental",
  "meta:createdDate": "2022-05-18",
  "meta:xdmType": "object",
  "properties": {
    "payerID": {
      "Title": "Payer ID",
      "type": "string",
      "description": "Unique identifier of the payer.",
      "meta:xdmType": "string",
      "meta:xdmField": "xdm:payerID",
      "tagNumber": 1
    },
    "contractID": {
      "Title": "Contract ID",
      "type": "string",
      "description": "Unique identifier of the contract that is signed by the member.",
      "meta:xdmType": "string",
      "meta:xdmField": "xdm:contractID",
      "tagNumber": 2
    },
    "ownerID": {
      "Title": "Owner ID",
      "type": "string",
      "description": "Unique identifier of the Owner of the policy which could be owned by an individual , group or an organization.",
      "meta:xdmType": "string",
      "meta:xdmField": "xdm:ownerID",
      "tagNumber": 3
    },
    "network": {
      "Title": "Network",
      "type": "string",
      "description": "The insurer-specific identifier for the insurer-defined network of providers to which the beneficiary may seek treatment which will be covered at the 'in-network' rate, otherwise 'out of network' term.",
      "meta:xdmType": "string",
      "meta:xdmField": "xdm:network",
      "tagNumber": 4
    },
    "affiliation": {
      "Title": "Affiliation",
      "type": "string",
      "description": "An affiliation to a government service, such as the army or navy.",
      "meta:xdmType": "string",
      "meta:xdmField": "xdm:affiliation",
      "tagNumber": 5
    },
    "coverageType": {
      "Title": "Coverage Type",
      "type": "string",
      "enum": [
        "Medical",
        "Dental",
        "Vision",
        "Accident"
      ],
      "description": "Member plan coverage type.",
      "meta:xdmType": "string",
      "meta:xdmField": "xdm:coverageType",
      "tagNumber": 6
    },
    "coverageStartDate": {
      "Title": "Coverage Start Date",
      "type": "string",
      "format": "date-time",
      "description": "Coverage start date.",
      "meta:xdmType": "date-time",
      "meta:xdmField": "xdm:coverageStartDate",
      "tagNumber": 7
    },
    "coverageEndDate": {
      "Title": "Coverage End Date",
      "type": "string",
      "format": "date-time",
      "description": "Coverage end date.",
      "meta:xdmType": "date-time",
      "meta:xdmField": "xdm:coverageEndDate",
      "tagNumber": 8
    },
    "lastVerification": {
      "Title": "Last Verification Date",
      "type": "string",
      "format": "date-time",
      "description": "The date on which this plan was last verified.",
      "meta:xdmType": "date-time",
      "meta:xdmField": "xdm:lastVerification",
      "tagNumber": 9
    },
    "cost": {
      "Title": "Plan Cost",
      "type": "number",
      "description": "The cost associated to this insurance plan.",
      "meta:xdmType": "number",
      "meta:xdmField": "xdm:cost",
      "tagNumber": 10
    }
  },
  "meta:xdmId": "https://ns.adobe.com/xdm/mixins/healthcare-plan-details",
  "meta:altId": "_xdm.mixins.healthcare-plan-details",
  "id": "_xdm.mixins.healthcare-plan-details"
}